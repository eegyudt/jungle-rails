
<h1 class=login_title>Login</h1>

<%= form_tag '/login' do %>

<span class="shadow-lg p-3 mb-5 bg-white rounded dash-link">
  <span>  Email: <%= text_field_tag :email %></span>
  <span>  Password: <%= password_field_tag :password %></span>
  <span  >  <%= submit_tag "Submit" %></span>
</span>
<% end %>